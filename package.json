  ],
  "license": "ISC",
  "repository": "ssh://git@github.com:gaui/fridagar-node.git",
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "dev": "bun test --watch",
